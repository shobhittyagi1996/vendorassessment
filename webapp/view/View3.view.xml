<mvc:View
    controllerName="com.sap.vendorassessment.controller.View3"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:tnt="sap.tnt"
    xmlns:f="sap.ui.layout.form"
    xmlns:upload="sap.m.upload"
    height="100%"
    xmlns:uxap="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:semantic="sap.f.semantic"
    xmlns:u="sap.ui.unified"
>
    <NavContainer id="wizardNavContainer">
        <pages>
            <Page
                id="wizardContentPage"
                showHeader="false"
            >
                <content>
                    <uxap:ObjectPageLayout
                        id="ObjectPageLayout"
                        upperCaseAnchorBar="false"
                        showAnchorBar="true"
                        enableLazyLoading="true"
                    >
                        <uxap:sections>
                            <uxap:ObjectPageSection
                                id="_IDGenObjectPageSection3"
                                showTitle="false"
                            >
                                <uxap:subSections>
                                    <uxap:ObjectPageSubSection
                                        id="_IDGenObjectPageSubSection3"
                                        title="Questions"
                                    >
                                        <uxap:blocks>
                                            <Table
                                                id="detailTable"
                                                items="{uModel>/}"
                                            >
                                                <columns>
                                                    <Column id="_IDGenColumn1">
                                                        <Text
                                                            id="_IDGenText11"
                                                            text="questions"
                                                        />
                                                    </Column>
                                                    <Column id="_IDGenColumn2">
                                                        <Text
                                                            id="_IDGenText12"
                                                            text="Supplier Response"
                                                        />
                                                    </Column>
                                                </columns>
                                                <items>
                                                    <ColumnListItem
                                                        id="_IDGenColumnListItem1"
                                                    >
                                                        <cells>
                                                            <Text
                                                                id="_IDGenText14"
                                                                text="{uModel>question}"
                                                            />
                                                            <RadioButtonGroup
                                                                id="GroupA"
                                                            >
                                                                <RadioButton
                                                                    id="_IDGenRadioButton1"
                                                                    text="Yes"
                                                                    editable="false"
                                                                />
                                                                <RadioButton
                                                                    id="_IDGenRadioButton2"
                                                                    text="No"
                                                                    editable="false"
                                                                />
                                                            </RadioButtonGroup>
                                                        </cells>
                                                    </ColumnListItem>
                                                </items>
                                            </Table>
                                        </uxap:blocks>
                                    </uxap:ObjectPageSubSection>
                                    <uxap:ObjectPageSubSection
                                        id="_IDGenObjectPageSubSection13"
                                        title="Attachments"
                                    >
                                        <uxap:blocks>
                                            <upload:UploadSetwithTable
                                                id="UploadSetTable1"
                                                sticky="ColumnHeaders"
                                                width="auto"
                                                fixedLayout="false"
                                                inset="true"
                                                autoPopinMode="true"
                                                hiddenInPopin="None"
                                                itemRenamed="onDocumentRenamedSuccess"
                                                mediaTypes="text/plain,application/msword,image/png"
                                                maxFileNameLength="60"
                                                uploadUrl="{= null }"
                                                multiple="false"
                                                items="{sModel>/}"
                                                mode="None"
                                            >
                                                <upload:columns>
                                                    <Column
                                                        id="filName"
                                                        importance="High"
                                                    >
                                                        <header>
                                                            <Label
                                                                id="_IDGenLa5"
                                                                text="file Name"
                                                            />
                                                        </header>
                                                    </Column>
                                                    <Column id="id">
                                                        <header>
                                                            <Label
                                                                id="_IDGenLabel7"
                                                                text="Type"
                                                            />
                                                        </header>
                                                    </Column>
                                                    <Column
                                                        id="actionButton"
                                                        importance="High"
                                                    />
                                                </upload:columns>
                                                <upload:items>
                                                    <upload:UploadSetwithTableItem
                                                        id="_IDGenUploadSetwithTableItem1"
                                                        fileName="{sModel>filename}"
                                                        mediaType="{sModel>mediaType}"
                                                    >
                                                        <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                                        <upload:cells>
                                                            <HBox
                                                                id="_IDGenHBox2"
                                                            >
                                                                <core:Icon
                                                                    id="_IDGenIcon1"
                                                                    src="{sModel>mediaType}"
                                                                    color="white"
                                                                    class="sapMUSTItemImage sapMUSTItemIcon"
                                                                />
                                                                <VBox
                                                                    id="_IDGenVBox1"
                                                                    class="sapUiTinyMargin sapUiSmallMarginBegin"
                                                                >
                                                                    <Link
                                                                        id="_IDGenLink3"
                                                                        text="{sModel>filename}"
                                                                        press="openPreview"
                                                                        class="sapUiTinyMarginBottom"
                                                                    />
                                                                    <Text
                                                                        id="_IDGenText10"
                                                                        class="sapUiTinyMarginTop"
                                                                    />
                                                                </VBox>
                                                            </HBox>
                                                            <Text
                                                                id="_IDGenLink2"
                                                                text="{sModel>type}"
                                                            />
                                                            <Button
                                                                id="_IDGenButton2"
                                                                class="sapUiTinyMarginBegin"
                                                                type="Transparent"
                                                                icon="sap-icon://decline"
                                                                press="onRemoveHandler"
                                                            />
                                                        </upload:cells>
                                                    </upload:UploadSetwithTableItem>
                                                </upload:items>
                                            </upload:UploadSetwithTable>
                                        </uxap:blocks>
                                    </uxap:ObjectPageSubSection>
                                </uxap:subSections>
                            </uxap:ObjectPageSection>
                        </uxap:sections>
                    </uxap:ObjectPageLayout>
                </content>
            </Page>
        </pages>
    </NavContainer>
</mvc:View>
