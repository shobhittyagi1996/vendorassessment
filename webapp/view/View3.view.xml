<mvc:View
    controllerName="com.sap.vendorassessment.controller.View3"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:upload="sap.m.upload"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <NavContainer id="wizardNavContainer">
        <pages>
            <Page
                id="wizardContentPage"
                showHeader="false"
            >
                <content>
                    <Wizard
                        id="CreateProductWizard"
                        class="sapUiResponsivePadding--header sapUiResponsivePadding--content"
                        complete="wizardCompletedHandler"
                    >
                        <WizardStep
                            id="selectinfo"
                            validated="true"
                            title="Select Questions for Assessment"
                            activate="additionalInfoValidation"
                        >
                            <Table
                                id="idSupplierTable"
                                items="{uModel>/}"
                                autoPopinMode="true"
                            >
                                <headerToolbar>
                                    <Toolbar id="_IDGenToolbar1">
                                        <HBox
                                            id="_IDGenHBox1"
                                            alignItems="Center"
                                            width="50%"
                                            justifyContent="SpaceAround"
                                        />
                                    </Toolbar>
                                </headerToolbar>
                                <columns>
                                    <Column id="_IDGenColumn1">
                                        <Text
                                            id="_IDGenText2"
                                            text="Questions"
                                        />
                                    </Column>

                                    <Column id="_IDGenColumn21">
                                        <Text
                                            id="_IDGenText12"
                                            text="Response Type"
                                        />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
                                        <cells>
                                            <Text
                                                id="_IDGenText6"
                                                text="{uModel>question}"
                                            />
                                            <RadioButtonGroup id="GroupA">
                                                <RadioButton
                                                    id="_IDGenRadioButton1"
                                                    text="Yes"
                                                    selected="true"
                                                />
                                                <RadioButton
                                                    id="_IDGenRadioButton2"
                                                    text="No"
                                                />
                                            </RadioButtonGroup>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                            <Toolbar id="_IDGenToolbar2">
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                <Button
                                    id="_IDGenButton1"
                                    text="Save"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                        <WizardStep
                            id="selectinfo1"
                            validated="true"
                            title="Attachments"
                            activate="additionalInfoValidation"
                        >
                            <upload:UploadSetwithTable
                                            id="UploadSetTable"
                                            sticky="ColumnHeaders,HeaderToolbar"
                                            width="auto"
                                            fixedLayout="false"
                                            inset="true"
                                            autoPopinMode="true"
                                            hiddenInPopin="None"
                                            uploadCompleted="onUploadCompleted"
                                            beforeInitiatingItemUpload="onBeforeInitiatingItemUpload"
                                            itemValidationHandler=".itemValidationCallback"
                                            beforeUploadStarts="onBeforeUploadStarts"
                                            selectionChange="onSelectionChange"
                                            itemRenamed="onDocumentRenamedSuccess"
                                            mediaTypes="text/plain,application/msword,image/png"
                                            maxFileNameLength="60"
                                            uploadUrl="/uploadFiles"
                                            multiple="true"
                                            items="{sModel>/}"
                                            mode="MultiSelect"
                                        >
                                            <upload:headerToolbar>
                                                <OverflowToolbar
                                                    id="_IDGenOverflowToolbar1"
                                                >
                                                    <Title
                                                        id="_IDGenTitle1"
                                                        level="H2"
                                                    />
                                                    <ToolbarSpacer
                                                        id="_IDGenToolbarSpacer3"
                                                    />
                                                    <SearchField
                                                        id="_IDGenSearchField2"
                                                        width="20%"
                                                        liveChange=".onSearch"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData3"
                                                                priority="NeverOverflow"
                                                            />
                                                        </layoutData>
                                                    </SearchField>
                                                    <ToolbarSeparator
                                                        id="_IDGenToolbarSeparator1"
                                                    />
                                                    <upload:ActionsPlaceholder
                                                        id="_IDGenActionsPlaceholder1"
                                                        placeholderFor="UploadButtonPlaceholder"
                                                    />
                                                    <Button
                                                        id="downloadSelectedButton"
                                                        text="Download"
                                                        enabled="false"
                                                        press="onDownloadFiles"
                                                    />
                                                    <MenuButton
                                                        id="_IDGenMenuButton1"
                                                        text="Add"
                                                    >
                                                        <menu>
                                                            <Menu
                                                                id="_IDGenMenu1"
                                                            >
                                                                <MenuItem
                                                                    id="_IDGenMenuItem1"
                                                                    text="URL"
                                                                    press="openAddOrEditDialog"
                                                                />
                                                                <MenuItem
                                                                    id="_IDGenMenuItem2"
                                                                    text="Document Without File"
                                                                    press=".openDocumentWithoutFileDialog"
                                                                />
                                                            </Menu>
                                                        </menu>
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData4"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </MenuButton>
                                                    <Button
                                                        id="removeDocumentButton"
                                                        text="Remove"
                                                        press=".onRemoveButtonFromMenuDocumentHandler"
                                                        enabled="false"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData5"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                    <Button
                                                        id="renameButton"
                                                        enabled="false"
                                                        press="onRenameDocument"
                                                        text="Rename"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData6"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                    <Button
                                                        id="editUrlButton"
                                                        text="Edit URL"
                                                        enabled="false"
                                                        press="onEditUrl"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData7"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                </OverflowToolbar>
                                            </upload:headerToolbar>
                                            <upload:columns>
                                                <Column
                                                    id="filName"
                                                    importance="High"
                                                >
                                                    <!-- <header>
                                                        <Label
                                                            id="_IDGenLa5"
                                                            text="file Name"
                                                        />
                                                    </header> -->
                                                </Column>
                                                <!-- <Column
                                                                id="fileName"
                                                                importance="High"
                                                            >
                                                                <header>
                                                                    <Label
                                                                        id="_IDGenLabel6"
                                                                        text="Name"
                                                                    />
                                                                </header>
                                                            </Column> -->
                                                <Column id="id">
                                                    <!-- <header>
                                                        <Label
                                                            id="_IDGenLabel7"
                                                            text="Type"
                                                        />
                                                    </header> -->
                                                </Column>

                                                <Column
                                                    id="actionButton"
                                                    importance="High"
                                                />
                                            </upload:columns>
                                            <upload:items>
                                                <upload:UploadSetwithTableItem
                                                    id="_IDGenUploadSetwithTableItem1"
                                                    fileName="{sModel>filename}"
                                                    mediaType="{sModel>mediaType}"
                                                >
                                                    <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                                    <upload:cells>
                                                        <HBox id="_IDGenHBox2">
                                                            <core:Icon
                                                                id="_IDGenIcon1"
                                                                src="{sModel>mediaType}"
                                                                color="white"
                                                                class="sapMUSTItemImage sapMUSTItemIcon"
                                                            />
                                                            <VBox
                                                                id="_IDGenVBox1"
                                                                class="sapUiTinyMargin sapUiSmallMarginBegin"
                                                            >
                                                                <Link
                                                                    id="_IDGenLink3"
                                                                    text="{sModel>filename}"
                                                                    press="openPreview"
                                                                    class="sapUiTinyMarginBottom"
                                                                />
                                                                <Text
                                                                    id="_IDGenText10"
                                                                    class="sapUiTinyMarginTop"
                                                                />
                                                            </VBox>
                                                        </HBox>
                                                        <!-- <Text
                                                                        id="_IDGen"
                                                                        text="{sModel>filename}"
                                                                    /> -->

                                                        <!-- <Text
                                                                        id="_IDGenText11"
                                                                        text="{sModel>name}"
                                                                    /> -->
                                                        <Text
                                                            id="_IDGenLink2"
                                                            text="{sModel>type}"
                                                        />

                                                        <Button
                                                            id="_IDGenButton2"
                                                            class="sapUiTinyMarginBegin"
                                                            type="Transparent"
                                                            icon="sap-icon://decline"
                                                            press="onRemoveHandler"
                                                        />
                                                    </upload:cells>
                                                </upload:UploadSetwithTableItem>
                                            </upload:items>
                                        </upload:UploadSetwithTable>
                        </WizardStep>
                    </Wizard>
                </content>
            </Page>
        </pages>
    </NavContainer>
</mvc:View>
